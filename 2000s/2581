#this is wrong code

import java.util.Scanner;

public class Main {
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int M,N;
		M=sc.nextInt();
		N=sc.nextInt();
		minority(M,N);
	}
	
	private static void minority(int M, int N){
		int i=2, index = 0, sum = 0;
		int[] data = new int[N];
		
		if(M>10000||N>10000||M>N)
			return;
		if(M==1)
			data[index++]=M++;
		if(!(M>N)&&M==2)
			data[index++]=M++;
		
		//소수 구하기
		for(;M<=N;M++){
			for(;i<M;i++){
				if(M%i==0)
					break;
			}
			if(i==M){
				data[index++]=i;
			}
			i=2;
		}
		
		index--;
		
		for(;index>=0;index--){
			sum+=data[index];
		}
		
		if(data[0]!=0){
			System.out.println(sum);
			System.out.println(data[0]);
		}else{
			System.out.println(-1);
		}
	}
}
